<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DragBubbleView">
        <attr name="bubbleRadius" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>
    <declare-styleable name="WaveView">
        <attr name="waveHeight" format="dimension"/>
        <attr name="waveCount" format="integer"/>
        <attr name="waveColor" format="color"/>
        <attr name="waveBackgroudColor" format="color"/>
        <attr name="waveBackgroudShape" format="integer"/>
        <attr name="bgHeight" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="custom_wave_view_attr">

        <attr name="circle_color" format="color"></attr>
        <attr name="circle_background_color" format="color"></attr>
        <attr name="progress_wave_color" format="color"></attr>
        <attr name="progress_text_size" format="dimension"></attr>
        <attr name="progress_text_color" format="color"></attr>

    </declare-styleable>

    <declare-styleable name="ArcView">
        <attr name="bg_circle_color" format="color"/>
        <attr name="circle_size" format="integer"/>
        <attr name="arc_color" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="integer"/>
    </declare-styleable>
</resources>